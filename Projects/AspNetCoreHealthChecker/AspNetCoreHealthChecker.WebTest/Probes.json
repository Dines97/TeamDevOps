{
  "IgnoreUnsupportedProbes": true,
  "Plugins": [
    "AspNetCoreHealthChecker.RabbitMQ",
    "AspNetCoreHealthChecker.Network",
    "AspNetCoreHealthChecker.Uri"
  ],
  "Probes": [
    {
      "Name": "SomeName",
      "Type": "HttpRequest",
      "Timeout": 1,
      "Severity": "Critical",
      "Description": "Some description",
      "Properties": {
        "Path": "http://localhost:5037/",
        "ReturnCode": 200
      }
    },
    {
      "Name": "RMQ",
      "Type": "RabbitMQ",
      "Timeout": 1,
      "Severity": "Critical",
      "Description": "Some description",
      "Properties": {
        "ConnectionString": "amqp://localhost:19420"
      }
    },
    {
      "Name": "DNS Resolve",
      "Type": "DnsResolve",
      "Timeout": 1,
      "Severity": "Critical",
      "Description": "Some description",
      "Properties": {
        "Host": "www.google.com",
        "TargetIP": "172.217.169.142"
      }
    },
    {
      "Name": "Tcp probe",
      "Type": "Tcp",
      "Timeout": 1,
      "Severity": "Critical",
      "Description": "Some description",
      "Properties": {
        "Host": "www.google.com",
        "Port": 80
      }
    },
    {
      "Name": "SSL probe",
      "Type": "SSL",
      "Timeout": 1,
      "Severity": "Critical",
      "Description": "Some description",
      "Properties": {
        "Host": "www.google.com",
        "Port": 80,
        "Expiration": 90
      }
    },
    {
      "Name": "URI probe",
      "Type": "uri",
      "Timeout": 1,
      "Severity": "Critical",
      "Description": "Some description",
      "Properties": {
        "Uri": "http://www.googdaskdjalkdjlaksjdlkasdjadsle.com"
      }
    },
    {
      "Name": "More URI probe",
      "Type": "uri2",
      "Timeout": 1,
      "Severity": "Critical",
      "Description": "Some description",
      "Properties": {
        "Uri": "http://www.googdaskdjalkdjlaksjdlkasdjadsle.com",
        "Test": [
          "http://www.dasdasddas.com",
          "http://dasdasdasd.tr"
        ]
      }
    }

